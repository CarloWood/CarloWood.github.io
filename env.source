function syncwww()
{
  cd /home/carlo/www
  cp /env.source howto/root-env.source
  cp /encrypted/cdeh/do_prompt howto/do_prompt
  cp /encrypted/cdeh/env.bashrc howto/env.bashrc
  cp /encrypted/cdeh/addhistory howto/addhistory
  rm -rf libecc
  cp -pr ../projects/libecc/libecc-objdir/doc libecc
  git add libecc
  git commit -a -m "Syncing www"
}
