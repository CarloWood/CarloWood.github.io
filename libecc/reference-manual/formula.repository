\form#191:\[ \begin{array}{lcl} c_s(i,n) &=& \sum_{j=i+1}^n\sum_{h=s+i+1-n}^s\sum_{r=h}^i\sum_{g=s-h+j-n}^{s-h}\sum_{z=g}^{j-1-i}\sum_{l=s-h-g}^{n-j} (-1)^{j+s-1}\eta(i,r)\eta(j-1-i,z)\eta(n-j,l)\binom{r}{h}\binom{n}{j}\binom{z}{g}\binom{l}{s-h-g}(i+1)^{z-g}(j+1)^{l-s+h+g}\\&&\\ &=& \sum_{j=i+1}^n\sum_{h=s+i+1-n}^s\sum_{r=h}^i\sum_{g=s-h+j-n}^{s-h}\sum_{z=g}^{j-1-i} (-1)^{n+h+g-1}\eta(i,r)\eta(j-1-i,z)\binom{r}{h}\binom{n}{j}\binom{z}{g}(i+1)^{z-g}(-1)^{n-j-s+h+g}\sum_{l=s-h-g}^{n-j}\eta(n-j,l)\binom{l}{s-h-g}(j+1)^{l-s+h+g}\\&&\\ &=& \sum_{j=i+1}^n\sum_{r=s+i+1-n}^i\sum_{h=s+i+1-n}^s\sum_{z=s-h+j-n}^{j-1-i}\sum_{g=s-h+j-n}^{s-h} (-1)^{n+h+g-1}\eta(i,r)\eta(j-1-i,z)\binom{r}{h}\binom{n}{j}\binom{z}{g}(i+1)^{z-g}(-1)^{n-j-s+h+g}\sum_{l=s-h-g}^{n-j}\eta(n-j,l)\binom{l}{s-h-g}(j+1)^{l-s+h+g}\\&&\\ &=& \sum_{j=i+1}^n\sum_{r=s+i+1-n}^i\sum_{z=j-n}^{j-1-i}\sum_{h=s-z+j-n}^s\sum_{g=s-h+j-n}^{s-h} (-1)^{n+h+g-1}\eta(i,r)\eta(j-1-i,z)\binom{r}{h}\binom{n}{j}\binom{z}{g}(i+1)^{z-g}(-1)^{n-j-s+h+g}\sum_{l=s-h-g}^{n-j}\eta(n-j,l)\binom{l}{s-h-g}(j+1)^{l-s+h+g}\\&&\\ &=& \sum_{j=i+1}^n\sum_{r=s+i+1-n}^i\sum_{z=j-n}^{j-1-i}\sum_{h=s-z+j-n}^s\sum_{f=0}^{n-j} (-1)^{n+s-f-1}\eta(i,r)\eta(j-1-i,z)\binom{r}{h}\binom{n}{j}\binom{z}{s-h-f}(i+1)^{z-s+h+f}(-1)^{n-j-f}\sum_{l=f}^{n-j}\eta(n-j,l)\binom{l}{f}(j+1)^{l-f}\\&&\\ \end{array} \]
\form#192:\[ \begin{array}{lcl} c_s(i,n) &=& \sum_{j=0}^{n-1-i}\sum_{r=0}^{n-1-s}\sum_{z=0}^{n-1-i}\sum_{h=0}^{z}\sum_{f=0}^{(n-1-i)-j}\sum_{l=0}^{((n-1-i)-j)-f} \\&&\\ && (-1)^{s+i+j}\eta(i,i-((n-1-s)-r))\eta(j,j-((n-1-i)-z))\eta((n-1-i)-j,l+f) \\&&\\ && \binom{s+r-(n-1-i)}{h+s-z}\binom{n}{j+i+1}\binom{z-((n-1-i)-j)}{z-h-f}\binom{l+f}{f}(i+1)^{h-(((n-1-i)-j)-f)}(j+i+2)^{l}\\&&\\ &=& \sum_{j=0}^{n-1-i} (-1)^{i+j} \binom{n}{j+i+1} \sum_{z=0}^{n-1-i} \eta(j,j-((n-1-i)-z)) \\&&\\ && \sum_{h=0}^{z} \sum_{f=0}^{(n-1-i)-j} \binom{z-((n-1-i)-j)}{z-h-f} (i+1)^{h-(((n-1-i)-j)-f)} \\&&\\ && \sum_{l=0}^{((n-1-i)-j)-f} \eta((n-1-i)-j,l+f) \binom{l+f}{f} (j+i+2)^{l} \\&&\\ && \sum_{r=0}^{n-1-s} (-1)^s \eta(i,i-((n-1-s)-r)) \binom{i-((n-1-s)-r)}{s-(z-h)} \end{array} \]
