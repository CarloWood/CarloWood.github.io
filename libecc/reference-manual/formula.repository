\form#107:\[ P_{i,n}(k) = \begin{cases} 0 & 0 \le k \le i \\ 1 & i < k \le n \\ \end{cases} \]
\form#108:\[ \begin{split} \sum_{j=i-n}^{i}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} &= (-1)^n \sum_{j=0}^{n}(-1)^{j}\binom{n}{j}\binom{k-1-i-((-1)j)}{n} = (-1)^n (-1)^n = 1 \end{split} \]
\form#109:\[ \begin{split} \sum_{j=i-n}^{k-n-1}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} &= 1 - \sum_{j=k-n}^{i}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} \end{split} \]
\form#110:\[ \sum_{j=i-n}^{k-n-1}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} = 1 + \sum_{j=i+1}^{k-n-1}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} = 1 \]
