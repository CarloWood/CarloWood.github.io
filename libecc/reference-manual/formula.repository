\form#112:\[ \begin{split} \sum_{j=i-n}^{k-n-1}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} &= \begin{cases} 0 & \text{if $k \le i$ (because then the sum is empty)} \\ 1 & \text{if $ k > i$} \end{cases} \\ &= H[k-i-1] \end{split} \]
\form#113:\[ \begin{split} P_{i,n}(k) &= \sum_{j=i-n}^{k-n-1}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} - \sum_{j=0}^{k-n-1}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} \\ &= \sum_{j=i-n}^{-1}(-1)^{n-(i-j)}\binom{n}{i-j}\binom{k-1-j}{n} \\ &= \sum_{j=0}^{n-1-i}(-1)^{j}\binom{n}{j}\binom{k-1-j-i+n}{n} \end{split} \]
