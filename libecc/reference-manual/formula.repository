\form#183:\[ \begin{array}{lcll} \eta(0, 0) &=& 1 \\&&\\ \eta(0, i) &=& 0 & \text{for any $i \ne 0$} \\&&\\ \eta(n, 0) &=& 0 & \text{for any $n \ne 0$} \\&&\\ \eta(n, i) &=& 0 & \text{if $n < i$} \\&&\\ \eta(n, i) &>& 0 & \text{iff $i \ge 0$ and $n \ge i$} \\&&\\ \eta(n, 1) &=& (n-1)! & \text{if $n > 0$} \\&&\\ \eta(n, n) &=& 1 & \text{for any $n \ge 0$} \\&&\\ \sum_{i=0}^n {\eta(n, i)} &=& n! \\&&\\ \sum_{i=0}^{a}{\eta(n, i)} &=& n! - \sum_{i=0}^n {\eta(n, i+a+1)} \\&&\\ \sum_{i=0}^n {\eta(n,i) \binom{i}{s}} &=& \eta(n+1,s+1) \end{array} \]
